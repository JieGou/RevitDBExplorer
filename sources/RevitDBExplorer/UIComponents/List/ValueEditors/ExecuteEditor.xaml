<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:RevitDBExplorer.WPF.Converters" 
                    xmlns:editors="clr-namespace:RevitDBExplorer.Domain.DataModel.ValueViewModels" 
                    >

    <DataTemplate DataType="{x:Type editors:ExecuteEditor}">
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
            <Button  Content="Execute"  Command="{Binding ValueViewModel.ExecuteCommand}"   Margin="5 0 0 0" Padding="7 2" MinWidth="120"/>
        </StackPanel>
    </DataTemplate>
    
</ResourceDictionary>